/*
Finonacci: recursive
*/

#include <iostream>
using namespace std;

class Solution {
public:
     int fibonacci(int n) {
         if (n < 1) return 0;
         if (n == 1 || n == 2) return 1;
         return fibonacci(n-1) + fibonacci(n-2);
     }
};

int main()
{
    Solution s;
    cout << "5: " << s.fibonacci(5) << endl;
    cout << "55: " << s.fibonacci(15) << endl;
}
